      "vite-plugin": [
        "lib/vite-plugin.d.ts"
      ],
      "ssr": [
        "ssr.d.ts"
      ]
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "esbuild": "^0.14.49",
    "micromatch": "^4.0.5",
    "node-fetch": "^3.2.6",
    "url-join": "^5.0.0",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.2",
    "vite": "^3.0.0"
  },
  "peerDependencies": {
    "vite": "^3.0.0"
  }
}
